{
  "name": "juristum-tma",
  "version": "0.9.5",
  "private": true,
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "npm run build:web && npm run build:bot",
    "build:web": "next build",
    "build:bot": "tsc -p tsconfig.bot.json",
    "start": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "start:bot": "node dist-bot/start.js",
    "postinstall": "prisma generate || true",
    "migrate": "prisma db push --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@mozilla/readability": "^0.5.0",
    "@prisma/client": "^5.22.0",
    "@twa-dev/sdk": "^7.10.1",
    "jsdom": "^24.1.3",
    "next": "15.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.33.5",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.2",
    "prisma": "^5.22.0",
    "typescript": "^5.9.3"
  }
}