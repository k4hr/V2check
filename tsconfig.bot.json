{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": false,
    "outDir": "dist-bot",
    "rootDir": ".",
    "strict": false,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": ["bot/**/*.ts"],
  "exclude": ["node_modules"]
}
